	      				</Map>
	      			</value>
	      		</entry>
	      		
	      		<!--  Config for Workflow `Example2Workflow` -->
	      		<entry key="Example2Workflow">
	      			<value>
	      				<Map>
	      					<entry key="type" value="sailpoint.object.Capability"/>
	      					<entry key="name" value="capabilityName"/>
	      				</Map>
	      			</value>
	      		</entry>
	      		
	      	  </Map>
	      	</value>
	      </entry>
	    </Map>
	  </Attributes>
	</Custom>

	

</sailpoint>



